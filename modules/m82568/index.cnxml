<m:mo>,</m:mo><m:msqrt>
<m:mn>6</m:mn>
</m:msqrt>
<m:mo stretchy="false">)</m:mo><m:mo>∪</m:mo><m:mo stretchy="false">(</m:mo><m:msqrt>
<m:mn>6</m:mn>
</m:msqrt>
<m:mo>,</m:mo><m:mi>∞</m:mi><m:mo stretchy="false">)</m:mo></m:mrow>
