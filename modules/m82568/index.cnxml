<entry align="center"><m:math>
 <m:mrow>
  <m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mi>x</m:mi><m:mo>|</m:mo><m:mi>x</m:mi><m:mo>&lt;</m:mo><m:mi>b</m:mi>
  </m:mrow> <m:mo>}</m:mo></m:mrow>
