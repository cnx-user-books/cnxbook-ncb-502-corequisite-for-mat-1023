 <m:mrow>
  <m:mrow><m:mo>[</m:mo> <m:mrow>
   <m:mi>a</m:mi><m:mo>,</m:mo><m:mi>∞</m:mi>
  </m:mrow> <m:mo>)</m:mo></m:mrow>
 </m:mrow>
</m:math></entry>
