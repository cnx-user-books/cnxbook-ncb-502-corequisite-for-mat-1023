<m:mi>x</m:mi>
<m:mn>2</m:mn>
</m:msup>
<m:mo>,</m:mo><m:mtext> </m:mtext></m:mrow>
</m:math>domain:<m:math>
<m:mrow>
<m:mtext> </m:mtext><m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mi>∞</m:mi><m:mo>,</m:mo><m:mn>0</m:mn><m:mo stretchy="false">)</m:mo><m:mo>∪</m:mo><m:mo stretchy="false">(</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mi>∞</m:mi><m:mo stretchy="false">)</m:mo></m:mrow>
</m:math>
</para>
</solution>
</exercise><exercise id="eip-88"><problem id="fs-id1165137810123">
<para id="fs-id1165137810125">Given <m:math>
<m:mrow>
<m:mtext> </m:mtext><m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>3</m:mn><m:msup>
