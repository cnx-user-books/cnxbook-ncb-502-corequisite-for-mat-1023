<para id="fs-id1165137770336"><m:math>
<m:mrow>
<m:mo stretchy="false">(</m:mo><m:mi>f</m:mi><m:mo>−</m:mo><m:mi>g</m:mi><m:mo stretchy="false">)</m:mo><m:mrow><m:mo>(</m:mo>
<m:mi>x</m:mi>
<m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mfrac>
<m:mrow>
<m:mn>4</m:mn><m:msup>
<m:mi>x</m:mi>
<m:mn>3</m:mn>
</m:msup>
<m:mo>+</m:mo><m:mn>8</m:mn><m:msup>
<m:mi>x</m:mi>
<m:mn>2</m:mn>
</m:msup>
