<m:mn>6</m:mn>
</m:msqrt>
<m:mo stretchy="false">)</m:mo><m:mo>∪</m:mo><m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:msqrt>
<m:mn>6</m:mn>
