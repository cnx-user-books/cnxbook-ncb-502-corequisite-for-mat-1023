</exercise><exercise id="eip-685"><problem id="fs-id1165137435040">
<para id="fs-id1165135174958">Given<m:math>
 <m:mrow>
  <m:mtext> </m:mtext><m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>2</m:mn><m:msup>
