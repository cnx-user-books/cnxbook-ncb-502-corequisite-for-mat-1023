     <m:mi>a</m:mi>
     <m:mrow>
      <m:mn>2</m:mn><m:mi>b</m:mi></m:mrow>
    </m:mfrac>
    <m:mo>−</m:mo><m:mfrac>
     <m:mrow>
      <m:mn>2</m:mn><m:mi>b</m:mi></m:mrow>
     <m:mrow>
      <m:mn>9</m:mn><m:mi>a</m:mi></m:mrow>
    </m:mfrac>
    </m:mrow>
   <m:mrow>
    <m:mfrac>
     <m:mrow>
      <m:mn>3</m:mn><m:mi>a</m:mi><m:mo>−</m:mo><m:mn>2</m:mn><m:mi>b</m:mi></m:mrow>
     <m:mrow>
      <m:mn>6</m:mn><m:mi>a</m:mi></m:mrow>
    </m:mfrac>
    </m:mrow>
  </m:mfrac>
  </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1167339343702"><para id="fs-id1167339343703"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mn>3</m:mn><m:mi>a</m:mi><m:mo>+</m:mo><m:mn>2</m:mn><m:mi>b</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mn>3</m:mn><m:mi>b</m:mi>
   </m:mrow>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></solution></exercise>
