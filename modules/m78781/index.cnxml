  <m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mi>q</m:mi><m:mo>−</m:mo><m:mn>3</m:mn><m:mi>p</m:mi><m:mo stretchy="false">)</m:mo><m:mo>(</m:mo><m:mn>16</m:mn><m:msup>
   <m:mi>q</m:mi>
