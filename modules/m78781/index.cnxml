<section id="fs-id1167339433907">
<title><link class="target-chapter" document="m51248">Rational Expressions</link></title>

<para id="fs-id1167339433916">For the following exercises, simplify the expression.</para>
<exercise id="fs-id1167339433919"><problem id="fs-id1167339433920"><para id="fs-id1167339433922"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>−</m:mo><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>12</m:mn>
   </m:mrow>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>−</m:mo><m:mn>8</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>16</m:mn>
   </m:mrow>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></problem> 
<solution id="fs-id1167339433980"><para id="fs-id1167339433981"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
   </m:mrow>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></solution></exercise>
<exercise id="fs-id1167339434014"><problem id="fs-id1167339434015"><para id="fs-id1167339434016"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mn>4</m:mn><m:msup>
     <m:mi>y</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>−</m:mo><m:mn>25</m:mn>
   </m:mrow>
   <m:mrow>
    <m:mn>4</m:mn><m:msup>
     <m:mi>y</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>−</m:mo><m:mn>20</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>25</m:mn>
   </m:mrow>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></problem></exercise> 
<exercise id="fs-id1167339434075"><problem id="fs-id1167339434076"><para id="fs-id1167339434077"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mn>2</m:mn><m:msup>
     <m:mi>a</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>−</m:mo><m:mi>a</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
   </m:mrow>
   <m:mrow>
    <m:mn>2</m:mn><m:msup>
     <m:mi>a</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>−</m:mo><m:mn>6</m:mn><m:mi>a</m:mi><m:mo>−</m:mo><m:mn>8</m:mn>
   </m:mrow>
  </m:mfrac>
  <m:mo>⋅</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>5</m:mn><m:msup>
     <m:mi>a</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>−</m:mo><m:mn>19</m:mn><m:mi>a</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
   </m:mrow>
   <m:mrow>
    <m:mn>10</m:mn><m:msup>
     <m:mi>a</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>−</m:mo><m:mn>13</m:mn><m:mi>a</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
   </m:mrow>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></problem> 
<solution id="fs-id1167339434201"><para id="fs-id1167339434202"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mn>1</m:mn>
   <m:mn>2</m:mn>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></solution></exercise>
<exercise id="fs-id1167339434220"><problem id="fs-id1167339434221"><para id="fs-id1167339434222"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mi>d</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
   </m:mrow>
   <m:mrow>
    <m:msup>
     <m:mi>d</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>−</m:mo><m:mn>9</m:mn>
   </m:mrow>
  </m:mfrac>
  <m:mo>⋅</m:mo><m:mfrac>
   <m:mrow>
    <m:mi>d</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
   </m:mrow>
   <m:mrow>
    <m:msup>
     <m:mi>d</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>−</m:mo><m:mn>16</m:mn>
   </m:mrow>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></problem></exercise> 
<exercise id="fs-id1167339434297"><problem id="fs-id1167339434298"><para id="fs-id1167339434299"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:msup>
     <m:mi>m</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>+</m:mo><m:mn>5</m:mn><m:mi>m</m:mi><m:mo>+</m:mo><m:mn>6</m:mn>
   </m:mrow>
   <m:mrow>
    <m:mn>2</m:mn><m:msup>
     <m:mi>m</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>−</m:mo><m:mn>5</m:mn><m:mi>m</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
   </m:mrow>
  </m:mfrac>
  <m:mo>÷</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>2</m:mn><m:msup>
     <m:mi>m</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>+</m:mo><m:mn>3</m:mn><m:mi>m</m:mi><m:mo>−</m:mo><m:mn>9</m:mn>
   </m:mrow>
   <m:mrow>
    <m:mn>4</m:mn><m:msup>
     <m:mi>m</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>−</m:mo><m:mn>4</m:mn><m:mi>m</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
   </m:mrow>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></problem> 
<solution id="fs-id1167339434421"><para id="fs-id1167339434422"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mi>m</m:mi><m:mo>+</m:mo><m:mn>2</m:mn>
   </m:mrow>
   <m:mrow>
    <m:mi>m</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
   </m:mrow>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></solution></exercise>
<exercise id="fs-id1167339434455"><problem id="fs-id1167339434456"><para id="fs-id1167339434457"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mn>4</m:mn><m:msup>
     <m:mi>d</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>−</m:mo><m:mn>7</m:mn><m:mi>d</m:mi><m:mo>−</m:mo><m:mn>2</m:mn>
   </m:mrow>
   <m:mrow>
    <m:mn>6</m:mn><m:msup>
     <m:mi>d</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>−</m:mo><m:mn>17</m:mn><m:mi>d</m:mi><m:mo>+</m:mo><m:mn>10</m:mn>
   </m:mrow>
  </m:mfrac>
  <m:mo>÷</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>8</m:mn><m:msup>
     <m:mi>d</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>+</m:mo><m:mn>6</m:mn><m:mi>d</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
   </m:mrow>
   <m:mrow>
    <m:mn>6</m:mn><m:msup>
     <m:mi>d</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>+</m:mo><m:mn>7</m:mn><m:mi>d</m:mi><m:mo>−</m:mo><m:mn>10</m:mn>
   </m:mrow>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></problem></exercise>
<exercise id="fs-id1167339434581"><problem id="fs-id1167339434582"><para id="fs-id1167339434583"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mn>10</m:mn>
   </m:mrow>
   <m:mi>x</m:mi>
  </m:mfrac>
  <m:mo>+</m:mo><m:mfrac>
   <m:mn>6</m:mn>
   <m:mi>y</m:mi>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></problem> 
<solution id="fs-id1167339434616"><para id="fs-id1167339434617"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mn>6</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>10</m:mn><m:mi>y</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:mi>y</m:mi>
   </m:mrow>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></solution></exercise>
<exercise id="fs-id1167339434652"><problem id="fs-id1167339434653"><para id="fs-id1167339434654"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mn>12</m:mn>
   </m:mrow>
   <m:mrow>
    <m:msup>
     <m:mi>a</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>+</m:mo><m:mn>2</m:mn><m:mi>a</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
   </m:mrow>
  </m:mfrac>
  <m:mo>−</m:mo><m:mfrac>
   <m:mn>3</m:mn>
   <m:mrow>
    <m:msup>
     <m:mi>a</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mn>−1</m:mn>
   </m:mrow>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></problem></exercise>
<exercise id="fs-id1167339434724"><problem id="fs-id1167339434725"><para id="fs-id1167339434726"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mfrac>
     <m:mn>1</m:mn>
     <m:mi>d</m:mi>
    </m:mfrac>
    <m:mo>+</m:mo><m:mfrac>
     <m:mn>2</m:mn>
     <m:mi>c</m:mi>
    </m:mfrac>
    
   </m:mrow>
   <m:mrow>
    <m:mfrac>
     <m:mrow>
      <m:mn>6</m:mn><m:mi>c</m:mi><m:mo>+</m:mo><m:mn>12</m:mn><m:mi>d</m:mi>
     </m:mrow>
     <m:mrow>
      <m:mi>d</m:mi><m:mi>c</m:mi>
     </m:mrow>
    </m:mfrac>
    
   </m:mrow>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></problem> 
<solution id="fs-id1167339434792"><para id="fs-id1167339434793"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mn>1</m:mn>
   <m:mn>6</m:mn>
  </m:mfrac>
  
 </m:mrow>
</m:math>
</para></solution></exercise>
<exercise id="fs-id1167339434811"><problem id="fs-id1167339434812"><para id="fs-id1167339434813"><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mfrac>
     <m:mn>3</m:mn>
     <m:mi>x</m:mi>
    </m:mfrac>
    <m:mo>−</m:mo><m:mfrac>
     <m:mn>7</m:mn>
