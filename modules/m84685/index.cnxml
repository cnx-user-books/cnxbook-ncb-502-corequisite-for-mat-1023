<m:mrow>
<m:mi>y</m:mi><m:mo>=</m:mo><m:mi>a</m:mi><m:msub>
<m:mrow>
<m:mi>log</m:mi>
</m:mrow>
<m:mi>a</m:mi>
</m:msub>
