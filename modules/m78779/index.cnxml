   <m:mi>a</m:mi>
   <m:mn>3</m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:msup>
   <m:mi>b</m:mi>
   <m:mn>3</m:mn>
  </m:msup>
  <m:mo>=</m:mo><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>+</m:mo><m:mi>b</m:mi><m:mo stretchy="false">)</m:mo><m:mo>(</m:mo><m:msup>
   <m:mi>a</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>−</m:mo><m:mi>a</m:mi><m:mi>b</m:mi><m:mo>+</m:mo><m:msup>
   <m:mi>b</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>)</m:mo>
 </m:mrow>
</m:math>
</entry>
</row>
<row>
<entry>difference of cubes</entry>
<entry><m:math>
 <m:mrow>
  <m:msup>
   <m:mi>a</m:mi>
   <m:mn>3</m:mn>
  </m:msup>
  <m:mo>−</m:mo><m:msup>
   <m:mi>b</m:mi>
   <m:mn>3</m:mn>
  </m:msup>
  <m:mo>=</m:mo><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>−</m:mo><m:mi>b</m:mi><m:mo stretchy="false">)</m:mo><m:mo>(</m:mo><m:msup>
   <m:mi>a</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:mi>a</m:mi><m:mi>b</m:mi><m:mo>+</m:mo><m:msup>
