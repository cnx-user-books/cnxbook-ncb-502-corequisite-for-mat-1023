    <entry nameend="c2" namest="c1">Multiply both sides by <m:math><m:mn>−7</m:mn></m:math>.</entry>
    <entry><media id="eip-id1169753238745" alt="."><image src="../../media/CNX_ElemAlg_Figure_02_02_005d_img_new.jpg" mime-type="image/png"/></media></entry>
  </row>
  <row>
    <entry nameend="c2" namest="c1">Multiply.</entry>
    <entry><media id="eip-id1169750595124" alt="."><image src="../../media/CNX_ElemAlg_Figure_02_02_005e_img_new.jpg" mime-type="image/png"/></media></entry>
