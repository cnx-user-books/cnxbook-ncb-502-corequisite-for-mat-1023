  <row>
    <entry nameend="c2" namest="c1">Simplify.</entry>
    <entry><media id="eip-id1169753102934" alt="."><image src="../../media/CNX_ElemAlg_Figure_02_01_007f_img_new.jpg" mime-type="image/png"/></media></entry>
  </row>
  <row>
    <entry>Check:</entry>
    <entry><media id="eip-id1169752915890" alt="."><image src="../../media/CNX_ElemAlg_Figure_02_01_007a_img_new.jpg" mime-type="image/png"/></media></entry>
    <entry/>
  </row>
  <row>
    <entry>Substitute <m:math><m:mi>a</m:mi><m:mo>=</m:mo><m:mn>−9</m:mn></m:math></entry>
    <entry><media id="eip-id1169753237111" alt="."><image src="../../media/CNX_ElemAlg_Figure_02_01_007b_img_new.jpg" mime-type="image/png"/></media></entry>
